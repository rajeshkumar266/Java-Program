class Solution {
    static boolean armstrongNumber(int n) {
        // code here
        int n1=0,digit=0;
        int temp = n ,temp2 =n;
        
        while(temp2 >0){
            digit++;
            int n2 = temp2/10;
        }
        
        while(n>0)
        {
            n=n%10;
            n1+=Math.pow(n,digit);
            n/=10;
        }
        return temp == n1;
    }
}
