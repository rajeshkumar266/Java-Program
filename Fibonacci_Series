package Problem_sloving;

import java.util.Scanner;
public class Fibonacci_Series { 
	
	public static void main(String args[]) {
		int c;
		int a=0;
		int b=1;
	Scanner sc = new Scanner(System.in);
	
	System.out.println("Enter a Number: ");
	int n = sc.nextInt();
	
	System.out.println("Fiboncci Series Values: " + a);
	System.out.println("Fiboncci Series Values: " + b);
	
	n=n-2;
	while(n>0)
	{
		c= a+b;
		System.out.println("Fiboncci Series Values: " + c);
		a=b;
		b=c;
		n--;
		
	}
	
}
}

