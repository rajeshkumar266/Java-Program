
//--------------------------------|  Finding Maximum Array Values   |  ----------------------------------------------


package Max_Array;

public class Maximum_Array {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int a[] = {3,7,9,2,5};
		
		int max = a[0]; 
		
		for(int i = 0;i<a.length;i++)
		{
			if(max<a[i])
			{
				if(max>a[i])
				a[i]=max;
				
				System.out.println("Maximum Array for Arrays : " + a[i]);
			}
				
				
		}

	}

}
